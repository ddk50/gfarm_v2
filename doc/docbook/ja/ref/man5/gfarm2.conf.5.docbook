<varlistentry>
<term>&lt;spool_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"spool" &lt;pathname&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;spool_server_listen_address_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"spool_server_listen_address" &lt;ipv4_address&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;spool_server_listen_backlog_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"spool_server_listen_backlog" &lt;number&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;spool_server_cred_type_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"spool_server_cred_type" &lt;cred_type&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;spool_server_cred_service_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"spool_server_cred_service" &lt;cred_service&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;spool_server_cred_name_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"spool_server_cred_name" &lt;cred_name&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;spool_check_level_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"spool_check_level" &lt;spck_level&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;metadb_server_host_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"metadb_server_host" &lt;hostname&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;metadb_server_port_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"metadb_server_port" &lt;portnumber&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;metadb_server_cred_type_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"metadb_server_cred_type" &lt;cred_type&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;metadb_server_cred_service_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"metadb_server_cred_service" &lt;cred_service&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;metadb_server_cred_name_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"metadb_server_cred_name" &lt;cred_name&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;metadb_server_listen_backlog_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"metadb_server_listen_backlog" &lt;number&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;metadb_server_max_descriptors_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"metadb_server_max_descriptors" &lt;number&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;metadb_server_stack_size_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"metadb_server_stack_size" &lt;number&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;metadb_server_thread_pool_size_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"metadb_server_thread_pool_size" &lt;number&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;metadb_server_job_queue_length_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"metadb_server_job_queue_length" &lt;number&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;metadb_server_heartbeat_interval_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"metadb_server_heartbeat_interval" &lt;number&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;metadb_server_dbq_size_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"metadb_server_dbq_size" &lt;number&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;ldap_server_host_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"ldap_server_host" &lt;hostname&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;ldap_server_port_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"ldap_server_port" &lt;portnumber&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;ldap_base_dn_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"ldap_base_dn" &lt;string&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;ldap_bind_dn_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"ldap_bind_dn" &lt;string&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;ldap_bind_password_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"ldap_bind_password" &lt;string&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;postgresql_server_host_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"postgresql_server_host" &lt;hostname&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;postgresql_server_port_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"postgresql_server_port" &lt;portnumber&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;postgresql_dbname_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"postgresql_dbname" &lt;string&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;postgresql_user_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"postgresql_user" &lt;string&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;postgresql_password_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"postgresql_password" &lt;string&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;postgresql_conninfo_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"postgresql_conninfo" &lt;string&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;auth_statement&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"auth" &lt;validity&gt; &lt;auth_method&gt; &lt;hostspec&gt;</literallayout></listitem>
</varlistentry>

<varlistentry>
<term>&lt;auth_command&gt; ::=</term>
<listitem><literallayout format="linespecific" class="normal">"enable" | "disable"</literallayout></listitem>
</varlistentry>

