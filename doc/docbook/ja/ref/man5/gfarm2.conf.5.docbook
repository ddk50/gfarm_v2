	&lt;ldap_bind_dn_statement&gt; |
	&lt;ldap_bind_password_statement&gt; |
	&lt;postgresql_server_host_statement&gt; |
	&lt;postgresql_server_port_statement&gt; |
	&lt;postgresql_dbname_statement&gt; |
