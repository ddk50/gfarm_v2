#!/bin/sh
# $Id: initheaders 6389 2012-07-02 05:12:52Z devtty $

if test ! -r ./configure.ac; then
    exit 1
fi

cd ./src/include/nata || exit 1

out=./.headers
rm -f ${out}
> ${out}

echo -n "HEADERS = " >> ${out}
for i in `ls *.h`; do
    echo ${i} | awk '{ printf("\\\n\t%s ", $1); }' >> ${out}
done

exit 0


